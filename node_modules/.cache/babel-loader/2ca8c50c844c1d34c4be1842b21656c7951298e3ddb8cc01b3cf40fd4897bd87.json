{"ast":null,"code":"export default {\n  name: \"AnswerTypeComponent\",\n  emits: [\"answer-type\"],\n  data() {\n    return {\n      answerType: \"\",\n      options: [{\n        text: \"Single\",\n        value: \"SINGLE\"\n      }, {\n        text: \"Multiple\",\n        value: \"MULTI\"\n      }]\n    };\n  },\n  methods: {\n    answerChange(data) {\n      data = this.answerType;\n      console.log(\"In answer type\", data);\n      this.$emit(\"answer-type\", data);\n    }\n  }\n};","map":{"version":3,"mappings":"AAaA;EACAA;EACAC;EACAC;IACA;MACAC;MACAC,UACA;QAAAC;QAAAC;MAAA,GACA;QAAAD;QAAAC;MAAA;IAEA;EACA;EACAC;IACAC;MACAN;MACAO;MACA;IACA;EACA;AACA","names":["name","emits","data","answerType","options","text","value","methods","answerChange","console"],"sourceRoot":"src/components","sources":["AnswerTypeComponent.vue"],"sourcesContent":["<template>\n  <div class=\"answerType\">\n    <h1>Select Your Answer Type</h1>\n\n    <select v-model=\"answerType\" @change=\"answerChange\">\n      <option v-for=\"option in options\" :value=\"option.value\" :key=\"option.id\">\n        {{ option.text }}\n      </option>\n    </select>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"AnswerTypeComponent\",\n  emits: [\"answer-type\"],\n  data() {\n    return {\n      answerType: \"\",\n      options: [\n        { text: \"Single\", value: \"SINGLE\" },\n        { text: \"Multiple\", value: \"MULTI\" },\n      ],\n    };\n  },\n  methods: {\n    answerChange(data){\n      data = this.answerType;\n      console.log(\"In answer type\", data)\n      this.$emit(\"answer-type\", data)\n    }\n  }\n};\n</script>\n\n<style scoped>\n.answerType {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    flex-direction: column;\n    height: calc(90vh - 340px);\n}\n\nselect {\n  width: 295px;\n  height: 49px;\n  border: none;\n  margin-top: 10px;\n  border: 2px solid red;\n  border-radius: 5px;\n  font-size: 25px;\n}\n</style>\n"]},"metadata":{},"sourc