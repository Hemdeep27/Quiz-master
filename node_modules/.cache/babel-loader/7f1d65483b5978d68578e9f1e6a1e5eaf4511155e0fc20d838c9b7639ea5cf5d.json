{"ast":null,"code":"import { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"addContest\",\n  data() {\n    return {\n      contestName: \"\",\n      contestCategory: \"\",\n      totalMarks: 0,\n      quizType: \"\",\n      duration: 0\n    };\n  },\n  components: {\n    ...mapGetters[\"getAddContest\"]\n  },\n  methods: {\n    ...mapActions[\"ADD_CONTEST\"],\n    addContestHandler() {\n      const payload = {\n        approved: true,\n        contentCategory: this.contentCategory,\n        contestName: this.contestName,\n        durationOfContest: this.duration,\n        moderatorId: \"1\",\n        quizMasterId: \"1\",\n        quizType: this.quizType,\n        totalMarks: this.totalMarks\n      };\n      this.$store.dispatch(\"ADD_CONTEST\", {\n        payload: payload\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAyBA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACA;IACAC;MACA;QACAC;QACAC;QACAT;QACAU;QACAC;QACAC;QACAT;QACAD;MACA;MACA;QAAAW;MAAA;IACA;EACA;AACA","names":["name","data","contestName","contestCategory","totalMarks","quizType","duration","components","methods","addContestHandler","approved","contentCategory","durationOfContest","moderatorId","quizMasterId","payload"],"sourceRoot":"src/views","sources":["AddContestView.vue"],"sourcesContent":["<template>\n  <div class=\"addContest\">\n    <div class=\"contestLeft\">\n      <h2>Quiz</h2>\n      <h3>Add a Contest</h3>\n    </div>\n    <div class=\"contestRight\">\n      <form @submit.prevent=\"addContestHandler\">\n        <label>Contest Name</label> <br />\n        <input type=\"text\" v-model=\"contestName\"/> <br />\n        <label>Contest Category</label> <br />\n        <input type=\"text\" v-model=\"contestCategory\"/><br />\n        <label>Total Marks</label> <br />\n        <input type=\"number\" v-model=\"totalMarks\"/> <br />\n        <label>Quiz Type</label> <br />\n        <input type=\"text\" v-model=\"quizType\" /><br />\n        <label>Duration</label><br />\n        <input type=\"number\" v-model=\"duration\"/><br />\n        <button>Submit</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"addContest\",\n  data() {\n    return {\n      contestName: \"\",\n      contestCategory: \"\",\n      totalMarks: 0,\n      quizType: \"\",\n      duration: 0,\n    };\n  },\n  components: {\n    ...mapGetters[\"getAddContest\"],\n  },\n  methods: {\n    ...mapActions[\"ADD_CONTEST\"],\n    addContestHandler() {\n      const payload = {\n        approved: true,\n        contentCategory: this.contentCategory,\n        contestName: this.contestName,\n        durationOfContest: this.duration,\n        moderatorId: \"1\",\n        quizMasterId: \"1\",\n        quizType: this.quizType,\n        totalMarks: this.totalMarks,\n      };\n      this.$store.dispatch(\"ADD_CONTEST\", {payload: payload});\n    },\n  },\n};\n</script>\n\n<style scoped>\n.addContest {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-evenly;\n  margin: 40px;\n  /* height: 100vh; */\n  /* background-color: rgb(121, 200, 115); */\n}\n\n.contestLeft {\n    display: flex;\n    flex-direction: column;\n    /* justify-content: center; */\n    width: 400px;\n    background-color: rgb(113, 154, 242);\n}\n\n.contestRight {\n    display: flex;\n    justify-content: center;\n    width: 400px;\n    /* background-color: rgb(224, 126, 80); */\n}\n\nlabel {\n    font-size: 22px;\n    margin-bottom: 10px;\n}\n\ninput {\n    height: 45px;\n    width: 290px;\n    border: none;\n    margin-bottom: 10px;\n    box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 3px 0px, rgba(0, 0, 0, 0.06) 0px 1px 2px 0px;\n    border-radius: 3px;\n}\n\n\nbutton {\n    width: 7rem;\n    height: 2.5rem;\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}